{
    "plugins": [],
    "source": {
        "include": ["src"],
        "includePattern": ".js$",
        "excludePattern": "(node_modules|docs)"
    },
    "templates": {
        "cleverLinks": "false",
        "monospaceLinks": "false",
        "name": "RauxApp Documentation"
    },
    "opts": {
        "recurse": "true",
        "destination": "./doc",
        "template": "node_modules/tui-jsdoc-template"
    }
}